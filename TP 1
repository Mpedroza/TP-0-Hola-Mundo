#include <iostream>

using std::cout;
using std::cin;

int main ()
{
  /* Declaracion de variables*/
  int numero1;
  int numero2;
  int suma;
  
  cout << "Escriba el primer entero";
  cin >> numero 1
  
  cout << "Escriba el segundo entero";
  cin >> numero 2
  
  suma = numero1 + numero2;
  
  cout << "La suma de" numero1 << "+" << numero2 << "es:" << suma << /n;
  
  return 0;
}
